{
  "id": null,
  "title": "xsong.us 完整監控 Dashboard",
  "tags": ["xsong.us", "comprehensive", "monitoring", "system", "network"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "網站可用性",
      "type": "stat",
      "targets": [
        {
          "expr": "probe_success{job=\"xsong.us-monitor\",instance=\"https://xsong.us\"}",
          "legendFormat": "xsong.us 可用性",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "網站回應時間",
      "type": "stat",
      "targets": [
        {
          "expr": "probe_duration_seconds{job=\"xsong.us-monitor\",instance=\"https://xsong.us\"}",
          "legendFormat": "回應時間",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 3
              }
            ]
          },
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "HTTP 狀態碼",
      "type": "stat",
      "targets": [
        {
          "expr": "probe_http_status_code{job=\"xsong.us-monitor\",instance=\"https://xsong.us\"}",
          "legendFormat": "HTTP 狀態碼",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
